{
  "routes": [
    {
      "src": "/success.html",
      "dest": "/success.html"
    },
    {
      "src": "/success",
      "dest": "/success.html"
    },
    {
      "src": "/success.html",
      "dest": "/shop.html?payfast_success=true",
      "status": 302,
      "continue": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}